{
  "manifest_version": 2,
  "name": "Paragraph Replacer",
  "version": "0.001",
  "background": {
    "scripts": ["popup/popup.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup/content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_title": "A popup will come"
  }
}
